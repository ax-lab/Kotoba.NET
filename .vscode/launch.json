	"version": "0.2.0",
	"configurations": [
		{
			"name": "Lauch Web",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build",
			"program": "${workspaceFolder}/Kotoba.Web/bin/Debug/net6.0/Kotoba.Web.exe",
			"args": [],
			"cwd": "${workspaceFolder}/Kotoba.Web",
			"console": "internalConsole",
			"stopAtEntry": false,
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			}
		},
		{
			"name": "Debug Import",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build",
			"program": "${workspaceFolder}/Importer.CLI/bin/Debug/net6.0/Importer.CLI.exe",
			"args": ["import", "./data"],
			"cwd": "${workspaceFolder}",
			"console": "internalConsole",
			"stopAtEntry": false
		}
	]
}
